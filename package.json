{
	"name": "andry-exp-system",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"db:start": "docker compose start",
		"db:stop": "docker compose stop",
		"db:reset": "prisma migrate reset --force",
		"db:seed": "prisma migrate reset --force && tsx prisma/seed.ts",
		"db:push": "prisma db push",
		"db:migrate": "prisma migrate reset --force && prisma migrate dev && tsx prisma/seed.ts",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write ."
	},
	"devDependencies": {
		"@skeletonlabs/skeleton": "^2.9.0",
		"@skeletonlabs/tw-plugin": "^0.3.1",
		"@sveltejs/adapter-auto": "^3.1.1",
		"@sveltejs/kit": "^2.0.0",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "8.56.0",
		"@types/node": "^20.11.24",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.17",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"flowbite-svelte-icons": "^1.3.1",
		"postcss": "^8.4.33",
		"postcss-load-config": "^5.0.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.3.6",
		"ts-node": "^10.9.2",
		"tslib": "^2.4.1",
		"typescript": "^5.3.3",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@floating-ui/dom": "^1.6.3",
		"@lucia-auth/adapter-prisma": "^4.0.0",
		"@prisma/client": "^5.9.1",
		"@sveltejs/adapter-node": "^5.0.1",
		"flowbite-svelte-icons": "^1.3.1",
		"lucia": "^3.0.1",
		"oslo": "^1.1.0",
		"prisma": "^5.9.1",
		"tsx": "^4.7.1",
		"zod": "^3.22.4"
	}
}